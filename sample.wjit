func main() {
    var x = fib(10) in
    println(x);
    func_a(10);
}

func fib(n) {
    if (n < 2) {
        n;
    } else {
        fib(n - 1) + fib(n - 2);
    };
}

func func_a(x) {
    println(x);
    if (x <= 0) {
        x;
    } else {
        func_b(x - 1);
    };
}

func func_b(x) {
    println(x);
    if (x <= 0) {
        x;
    } else {
        func_a(x - 1);
    };
}
